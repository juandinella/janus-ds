{
  "name": "janus-ds",
  "version": "1.0.0",
  "description": "Janus Design System",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "husky install & npm run build",
    "tokens": "node ./scripts/build-tokens",
    "start": "npm run tokens && npm run storybook",
    "test": "jest src --ci",
    "test:watch": "jest --watch",
    "test:update": "jest --updateSnapshot",
    "test:coverage": "jest --coverage",
    "test:coverage-web": "npm run test:coverage && open coverage/lcov-report/index.html",
    "storybook": "storybook dev ./public -p 6006",
    "lint": "eslint src/ --ext .jsx --ext .js",
    "build-storybook": "storybook build",
    "build": "rm -rf dist & rollup -c --bundleConfigAsCjs",
    "cc": "node ./scripts/create-component"
  },
  "author": "Juan Di Nella <juan.dinella@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/juandinella/janus-ds"
  },
  "files": [
    "dist/",
    "public/",
    "package.json",
    "README.md"
  ],
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-proposal-throw-expressions": "^7.22.5",
