    "babel-plugin-styled-components": "^2.1.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "enquirer": "^2.4.1",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest-dom": "^5.1.0",
